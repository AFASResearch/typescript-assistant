{
  "name": "typescript-assistant",
  "version": "0.26.1",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "tsa": "./dist/index.js"
  },
  "scripts": {
    "prepublishOnly": "tsc -p ./src/tsconfig.json",
    "assist": "ts-node src/index.ts",
    "release": "ts-node src/index.ts release",
    "fix": "ts-node src/index fix",
    "clean": "ts-node src/index clean",
    "coverage-show": "opn build/coverage/index.html",
    "tslint": "tslint --project ./src/tsconfig.json ./src/**/*.ts && tslint --project ./tsconfig.json ./test/**/*.ts",
    "ci": "ts-node src/index ci",
    "postcheckout": "ts-node src/index post-checkout || exit 0",
    "postmerge": "ts-node src/index post-merge || exit 0",
    "precommit": "ts-node src/index pre-commit",
    "prepush": "ts-node src/index pre-push"
  },
  "author": "johan@johangorter.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AFASResearch/typescript-assistant"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "json",
      "html",
      "text-summary"
    ],
    "cache": true,
    "temp-directory": "./build/nyc/cache",
    "all": true,
    "check-coverage": false,
    "report-dir": "./build/coverage",
    "es-module": false,
    "lines": 1,
    "statements": 0.1,
    "functions": 0.1,
    "branches": 0,
    "watermarks": {
      "lines": [
        75,
        100
      ],
      "functions": [
        75,
        100
      ],
      "branches": [
        75,
        100
      ],
      "statements": [
        75,
        100
      ]
    }
  },
  "dependencies": {
    "@types/chai": "4.0.10",
    "@types/mocha": "2.2.45",
    "@types/node": "8.5.2",
    "async": "2.6.0",
    "chai": "4.1.2",
    "chokidar": "2.0.0",
    "cross-env": "5.1.3",
    "glob": "7.1.2",
    "handlebars": "4.0.11",
    "husky": "0.14.3",
    "inquirer": "4.0.2",
    "mocha": "4.1.0",
    "nyc": "11.4.1",
    "opn-cli": "3.1.0",
    "source-map-support": "0.5.0",
    "tree-kill": "1.2.0",
    "ts-node": "4.1.0",
    "tslib": "1.8.1",
    "tslint": "5.8.0",
    "tslint-microsoft-contrib": "5.0.1",
    "tslint-sonarts": "1.3.0",
    "tsutils": "2.15.0",
    "typescript": "2.6.2",
    "typescript-formatter": "7.0.1",
    "ws": "3.3.3",
    "yargs": "10.0.3"
  },
  "devDependencies": {
    "@types/async": "2.0.46",
    "@types/chokidar": "1.7.4",
    "@types/glob": "5.0.34",
    "@types/handlebars": "4.0.36",
    "@types/inquirer": "0.0.36",
    "@types/sinon": "4.1.2",
    "@types/ws": "3.2.1",
    "@types/yargs": "10.0.0",
    "sinon": "4.1.3"
  }
}
