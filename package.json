{
  "name": "typescript-assistant",
  "version": "0.12.1",
  "description": "",
  "main": "build/js/src/index.js",
  "bin": {
    "tsa": "./build/js/src/index.js"
  },
  "scripts": {
    "prepublishOnly": "tsc",
    "assist": "ts-node --fast src/index.ts",
    "release": "ts-node --fast src/index.ts release",
    "fix": "ts-node --fast src/index fix",
    "postcheckout": "ts-node --fast src/index post-checkout",
    "postmerge": "ts-node --fast src/index post-merge",
    "precommit": "ts-node --fast src/index pre-commit",
    "prepush": "cross-env TS_NODE_FAST=true nyc --check-coverage -- mocha --compilers ts:ts-node/register --reporter dot --recursive",
    "clean": "rimraf ./build"
  },
  "files": [
    "src",
    "build/js/src",
    "build/js/tools",
    "tools",
    "tsconfig.json",
    "tsfmt.json",
    "tslint.tsa.json",
    "tslint.json"
  ],
  "author": "johan@johangorter.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AFASResearch/typescript-assistant"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html",
      "text-summary"
    ],
    "cache": true,
    "temp-directory": "./build/nyc/cache",
    "all": true,
    "check-coverage": false,
    "report-dir": "./build/coverage",
    "es-module": false,
    "lines": 1,
    "statements": 1,
    "functions": 1,
    "branches": 1,
    "watermarks": {
      "lines": [
        75,
        100
      ],
      "functions": [
        75,
        100
      ],
      "branches": [
        75,
        100
      ],
      "statements": [
        75,
        100
      ]
    }
  },
  "dependencies": {
    "cross-env": "5.0.1",
    "glob": "7.1.2",
    "husky": "0.13.4",
    "inquirer": "3.1.1",
    "mocha": "3.4.2",
    "nyc": "11.0.2",
    "source-map-support": "0.4.15",
    "ts-node": "3.2.1",
    "tslib": "1.7.1",
    "tslint": "5.5.0",
    "tslint-microsoft-contrib": "5.0.1",
    "tsutils": "2.7.1",
    "typescript": "2.4.2",
    "typescript-formatter": "5.2.0"
  },
  "devDependencies": {
    "@types/chai": "4.0.0",
    "@types/chokidar": "1.7.1",
    "@types/glob": "5.0.30",
    "@types/inquirer": "0.0.35",
    "@types/mocha": "2.2.41",
    "@types/node": "8.0.1",
    "@types/sinon": "2.3.1",
    "chai": "4.0.2",
    "chokidar": "1.7.0",
    "rimraf": "2.6.1",
    "sinon": "2.3.5"
  }
}
