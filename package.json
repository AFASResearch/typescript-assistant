{
  "name": "typescript-assistant",
  "version": "0.24.4",
  "description": "",
  "main": "dist/index.js",
  "bin": {
    "tsa": "./dist/index.js"
  },
  "scripts": {
    "prepublishOnly": "tsc -p ./src/tsconfig.json",
    "assist": "ts-node --fast src/index.ts",
    "release": "ts-node --fast src/index.ts release",
    "fix": "ts-node --fast src/index fix",
    "clean": "ts-node --fast src/index clean",
    "coverage-show": "opn build/coverage/index.html",
    "tslint": "tslint --project ./src/tsconfig.json ./src/**/*.ts && tslint --project ./tsconfig.json ./test/**/*.ts",
    "ci": "ts-node --fast src/index ci",
    "postcheckout": "ts-node --fast src/index post-checkout || exit 0",
    "postmerge": "ts-node --fast src/index post-merge || exit 0",
    "precommit": "ts-node --fast src/index pre-commit",
    "prepush": "ts-node --fast src/index pre-push"
  },
  "author": "johan@johangorter.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AFASResearch/typescript-assistant"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "json",
      "html",
      "text-summary"
    ],
    "cache": true,
    "temp-directory": "./build/nyc/cache",
    "all": true,
    "check-coverage": false,
    "report-dir": "./build/coverage",
    "es-module": false,
    "lines": 1,
    "statements": 0.1,
    "functions": 0.1,
    "branches": 0,
    "watermarks": {
      "lines": [
        75,
        100
      ],
      "functions": [
        75,
        100
      ],
      "branches": [
        75,
        100
      ],
      "statements": [
        75,
        100
      ]
    }
  },
  "dependencies": {
    "@types/chai": "4.0.4",
    "@types/mocha": "2.2.44",
    "@types/node": "8.0.51",
    "async": "2.6.0",
    "chai": "4.1.2",
    "chokidar": "1.7.0",
    "cross-env": "5.1.1",
    "glob": "7.1.2",
    "husky": "0.14.3",
    "inquirer": "4.0.0",
    "mocha": "4.0.1",
    "nyc": "11.3.0",
    "opn-cli": "3.1.0",
    "source-map-support": "0.5.0",
    "tree-kill": "1.2.0",
    "ts-node": "3.3.0",
    "tslib": "1.8.0",
    "tslint": "5.8.0",
    "tslint-microsoft-contrib": "5.0.1",
    "tslint-sonarts": "1.1.1",
    "tsutils": "2.12.2",
    "typescript": "2.6.1",
    "typescript-formatter": "7.0.0",
    "ws": "3.3.1",
    "yargs": "10.0.3"
  },
  "devDependencies": {
    "@types/async": "2.0.45",
    "@types/chokidar": "1.7.3",
    "@types/glob": "5.0.33",
    "@types/inquirer": "0.0.35",
    "@types/sinon": "2.3.7",
    "@types/ws": "3.2.0",
    "@types/yargs": "8.0.2",
    "sinon": "4.1.2"
  }
}
