{
  "name": "typescript-assistant",
  "version": "0.8.10",
  "description": "",
  "main": "build/js/src/index.js",
  "bin": {
    "tsa": "./build/js/src/index.js"
  },
  "scripts": {
    "start": "ts-node --fast src/index.ts",
    "format": "ts-node --fast src/index format",
    "postcheckout": "ts-node --fast tools/githooks/post-checkout",
    "postmerge": "ts-node --fast tools/githooks/post-merge",
    "precommit": "ts-node --fast tools/githooks/pre-commit",
    "prepush": "cross-env TS_NODE_FAST=true nyc --check-coverage -- mocha --compilers ts:ts-node/register --reporter dot --recursive"
  },
  "files": [
    "src",
    "build/js/src",
    "build/js/tools",
    "tools",
    "tsconfig.json",
    "tsfmt.json",
    "tslint.tsa.json",
    "tslint.json"
  ],
  "author": "johan@johangorter.com",
  "license": "MIT",
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html",
      "text-summary"
    ],
    "cache": true,
    "temp-directory": "./build/nyc/cache",
    "all": true,
    "check-coverage": false,
    "report-dir": "./build/coverage",
    "es-module": false,
    "lines": 1,
    "statements": 1,
    "functions": 1,
    "branches": 1,
    "watermarks": {
      "lines": [
        75,
        100
      ],
      "functions": [
        75,
        100
      ],
      "branches": [
        75,
        100
      ],
      "statements": [
        75,
        100
      ]
    }
  },
  "dependencies": {
    "glob": "7.1.1",
    "inquirer": "3.0.1",
    "tslint-microsoft-contrib": "4.0.0",
    "source-map-support": "0.4.6"
  },
  "devDependencies": {
    "@types/chai": "3.4.34",
    "@types/glob": "5.0.30",
    "@types/inquirer": "0.0.32",
    "@types/mocha": "2.2.39",
    "@types/node": "7.0.5",
    "@types/sinon": "1.16.33",
    "chai": "3.5.0",
    "cross-env": "3.1.4",
    "husky": "0.13.1",
    "mocha": "3.2.0",
    "nyc": "10.1.2",
    "sinon": "1.17.7",
    "ts-node": "2.1.0",
    "tslint": "4.4.2",
    "typescript": "2.1.6",
    "typescript-formatter": "4.1.1"
  },
  "peerDependencies": {
    "typescript": ">=2.1.0",
    "typescript-formatter": ">=2.1.0",
    "tslint": ">=4.0.0"
  }
}
